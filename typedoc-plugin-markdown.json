{
    "$schema": "https://typedoc-plugin-markdown.org/schema.json",
    "classPropertiesFormat": "list",
    "customAnchorsFormat": "curlyBrace",
    "entryFileName": "README",
    "enumMembersFormat": "list",
    "excludeScopesInPaths": false,
    "expandObjects": true,
    "expandParameters": true,
    "favicon": "../../src/components/icons/icon.ico",
    "hideBreadcrumbs": false,
    "hidePageHeader": false,
    "hidePageTitle": false,
    "indexFormat": "list",
    "interfacePropertiesFormat": "list",
    "mergeReadme": true,
    "prettierConfigFile": "../../.prettierrc",
    "sanitizeComments": false,
    "tableColumnSettings": {
        "hideDefaults": false,
        "hideInherited": false,
        "hideModifiers": false,
        "hideOverrides": false,
        "hideSources": false,
        "hideValues": false,
        "leftAlignHeaders": false
    },
    "typeAliasPropertiesFormat": "list",
    "typeDeclarationFormat": "list",
    "typeDeclarationVisibility": "verbose",
    "useCodeBlocks": true,
    "useCustomAnchors": true,
    "useHTMLAnchors": false,
    "useHTMLEncodedBrackets": false
}
